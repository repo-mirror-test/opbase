<?xml version="1.0" encoding="UTF-8"?>
<config name="ops_base" copy_type="delivery" src_path="" dst_path="ops_base" install_type="all" install_mod="550" install_own="$username:$usergroup">
    <package_info>
        <product_name>cann</product_name>
        <func_name>opbase</func_name>
        <suffix>run</suffix>
        <install_script>share/info/ops_base/script/install.sh</install_script>
        <help>share/info/ops_base/script/help.info</help>
        <cleanup>share/info/ops_base/script/cleanup.sh</cleanup>
        <parallel>true</parallel>
    </package_info>
    <generate_info value="opbase_version.h" dst_path="share/info/ops_base" install_path="$(TARGET_ENV)/include/version" install_mod="440" install_type="devel" generator="version_header">
        <OPBASE_VERSION>$(ASCEND_VER)</OPBASE_VERSION>
        <OPBASE_TIMESTAMP>$(TIMESTAMP_NO)</OPBASE_TIMESTAMP>
    </generate_info>
    <generate_info value="scene.info" dst_path="share/info/ops_base" install_path="share/info/opbase" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <file_info value="ops_base_script" copy_type="source" src_path="" dst_path="share/info/ops_base" install_path="share/info/opbase">
        <file value="script" entity="true"/>
        <file value="bin" entity="true"/>
        <file value="version.info" dst_path="share/info/ops_base"/>
    </file_info>
    <file_info value="ops_base_script" copy_type="source" src_path="" dst_path="share/info/ops_base" install_path="opp">
        <file value="version.info" dst_path="share/info/ops_base"/>
    </file_info>
    <generate_info value="scene.info" dst_path="share/info/ops_base" install_path="opp" install_mod="440">
        <os>$(OS_NAME)</os>
        <os_version>$(OS_VER)</os_version>
        <arch>$(ARCH)</arch>
    </generate_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/include" install_path="$(TARGET_ENV)/include" install_type="all">
        <file value="aicpu_common" install_mod="550" entity="true"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/include/aicpu_common/context/cust_op" install_path="$(TARGET_ENV)/include/aicpu" install_type="all">
        <file value="cust_cpu_utils.h" install_mod="550"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/include/aicpu_common/context/cpu_proto" install_path="$(TARGET_ENV)/include/aicpu" install_type="all">
        <file value="cpu_attr_value.h" install_mod="550"/>
        <file value="cpu_tensor.h" install_mod="550"/>
        <file value="cpu_tensor_shape.h" install_mod="550"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/include/aicpu_common/context/common" install_path="$(TARGET_ENV)/include/aicpu" install_type="all">
        <file value="cpu_context.h" install_mod="550"/>
        <file value="cpu_types.h" install_mod="550"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/pkg_inc" install_path="$(TARGET_ENV)/pkg_inc" install_type="all">
        <file value="op_common" install_mod="550" entity="true"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/conf" install_path="$(TARGET_ENV)/conf" install_type="all">
        <file value="dump_tool_config.ini" install_mod="550" entity="true"/>
    </file_info>
    <file_info value="ops_base_lib" copy_type="delivery" src_path="" dst_path="ops_base/lib" install_path="$(TARGET_ENV)/lib64" install_mod="440">
        <file value="libops_base.so" file_type="shared"/>
    </file_info>
    <file_info value="ops_base_lib_aicpu" copy_type="delivery" src_path="" dst_path="ops_base/lib/aicpu_common" install_path="$(TARGET_ENV)/lib64" install_mod="440">
        <file value="libaicpu_context.a" file_type="shared"/>
        <file value="libaicpu_nodedef.a" file_type="shared"/>
        <file value="libbase_ascend_protobuf.a" file_type="shared"/>
        <file value="libaicpu_context_host.a" file_type="shared"/>
        <file value="libaicpu_nodedef_host.a" file_type="shared"/>
        <file value="libhost_ascend_protobuf.a" file_type="shared"/>
    </file_info>
    <file_info value="ops_base_lib_aicpu_cust" copy_type="delivery" src_path="" dst_path="ops_base/lib/aicpu_common" install_path="$(TARGET_ENV)/lib64/device/lib64" install_mod="440">
        <file value="libaicpu_cust_log.a" file_type="shared"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/aclnn" install_path="$(TARGET_ENV)/include/aclnn" install_type="all">
        <file value="acl_meta.h" install_mod="550"/>
        <file value="aclnn_base.h" install_mod="550"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/aclnn/opdev" install_path="$(TARGET_ENV)/include/aclnn/opdev" install_type="all">
        <file value="bfloat16.h" install_mod="550"/>
        <file value="common_types.h" install_mod="550"/>
        <file value="data_type_utils.h" install_mod="550"/>
        <file value="fast_vector.h" install_mod="550"/>
        <file value="format_utils.h" install_mod="550"/>
        <file value="fp16_t.h" install_mod="550"/>
        <file value="framework_op.h" install_mod="550"/>
        <file value="make_op_executor.h" install_mod="550"/>
        <file value="object.h" install_mod="550"/>
        <file value="op_arg_def.h" install_mod="550"/>
        <file value="op_cache_container.h" install_mod="550"/>
        <file value="op_cache.h" install_mod="550"/>
        <file value="op_config.h" install_mod="550"/>
        <file value="op_def.h" install_mod="550"/>
        <file value="op_dfx.h" install_mod="550"/>
        <file value="op_errno.h" install_mod="550"/>
        <file value="op_executor.h" install_mod="550"/>
        <file value="op_log.h" install_mod="550"/>
        <file value="platform.h" install_mod="550"/>
        <file value="pool_allocator.h" install_mod="550"/>
        <file value="shape_utils.h" install_mod="550"/>
        <file value="small_vector.h" install_mod="550"/>
        <file value="tensor_view_utils.h" install_mod="550"/>
    </file_info>
        <file_info copy_type="source" src_path="" dst_path="ops_base/aclnn/opdev/op_common" install_path="$(TARGET_ENV)/include/aclnn/opdev/op_common" install_type="all">
        <file value="data_type_utils.h" install_mod="550"/>
    </file_info>
    <file_info copy_type="source" src_path="" dst_path="ops_base/aclnn/opdev/aicpu" install_path="$(TARGET_ENV)/include/aclnn/opdev/aicpu" install_type="all">
        <file value="aicpu_any_value.h" install_mod="550"/>
        <file value="aicpu_args_handler.h" install_mod="550"/>
        <file value="aicpu_ext_info_handle.h" install_mod="550"/>
        <file value="aicpu_kernel_launcher.h" install_mod="550"/>
        <file value="aicpu_task.h" install_mod="550"/>
        <file value="aicpu_uitls.h" install_mod="550"/>
    </file_info>
    <file_info value="ops_base_lib" copy_type="delivery" src_path="" dst_path="ops_base/lib/aclnn" install_path="$(TARGET_ENV)/lib64" install_mod="440">
        <file value="libnnopbase.so" file_type="shared"/>
        <file value="libdummy_tls.so" file_type="shared"/>
    </file_info>
    <file_info value="stub" copy_type="delivery" dst_path="ops_base/stub" install_path="$(TARGET_ENV)/devlib" install_mod="440">
        <file value="libnnopbase.so" file_type="shared"/>
    </file_info>

    <file_info value="aclnnop_dir" copy_type="source" src_path="" dst_path="ops_base" install_path="$(TARGET_ENV)/include" install_type="all">
      <file value="aclnnop" install_mod="550"/>
    </file_info>

    <block_info dependtree="false" dst_path="ops_base" block_conf_path="ascend">
        <block name="EngineeringCommon"/>
        <block name="EngineeringFiles"/>
    </block_info>
    <dir_info value="ops_base" install_path="opbase" install_mod="550" install_type="all">
        <path value="share" install_mod="550"/>
        <path value="share/info" install_mod="750"/>
        <path value="share/info/opbase" install_mod="750"/>
        <path value="share/info/opbase/script" install_mod="550"/>
        <path value="share/info/opbase/bin" install_mod="550"/>
        <path value="$(TARGET_ENV)/lib64/device" install_mod="550"/>
        <path value="$(TARGET_ENV)/lib64/device/lib64" install_mod="550"/>
    </dir_info>
    <dir_info value="ops_base" install_path="" install_mod="550" install_type="all">
        <path value="opp" install_mod="750"/>
        <path value="opp/vendors" install_mod="750"/>
        <path value="$(TARGET_ENV)/include/aicpu_common" install_mod="550"/>
        <path value="$(TARGET_ENV)/include/aicpu" install_mod="550"/>
        <path value="$(TARGET_ENV)/pkg_inc/op_common" install_mod="550"/>
        <path value="$(TARGET_ENV)/include/aclnn" install_mod="550"/>
        <path value="$(TARGET_ENV)/include/aclnn/opdev" install_mod="550"/>
        <path value="$(TARGET_ENV)/include/aclnn/opdev/op_common" install_mod="550"/>
        <path value="$(TARGET_ENV)/include/aclnn/opdev/aicpu" install_mod="550"/>
    </dir_info>
</config>
